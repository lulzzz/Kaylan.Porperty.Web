@{
    ViewBag.Title = "Admin Dashboard";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<head>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Scripts/jQuery-2.2.0.min.js"></script>

    <script type="text/javascript">
$(document).ready(function () {
$('#btn').click(function () {
    $('#result').load('/User/TotalUser');
});
});

$(document).ready(function () {
    $('#btn1').click(function () {
        $('#result').load('/User/SalesList');
    });
});

$(document).ready(function () {
    $('#btn2').click(function () {
        $('#result').load('/User/RentList');
    });
});


$(document).ready(function () {
    $('#btn3').click(function () {
        $('#result').load('/User/RentList');
    });
});


    </script>

</head>
<section class="content-header">
    <h1>Admin Dashboard</h1>
</section>

<!-- Main content -->
<section class="content">
    <!-- Info boxes -->
    <div class="row">
        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">
                <div id="btn">
                    <span class="info-box-icon bg-aqua" style="cursor: pointer;"><i class="ion ion-ios-gear-outline"></i></span>
                </div>

                    <div class="info-box-content">
                    <span class="info-box-text">Total User</span>
                        <span class="info-box-number">@ViewBag.UserList<small></small></span>
                        
                      
                    </div>
                    <!-- /.info-box-content -->
                </div>
            <!-- /.info-box -->
        </div>
        <!-- /.col -->

        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">
                <div id="btn">
                    <span class="info-box-icon bg-red" style="cursor: pointer;"><i class="fa fa-google-plus"></i></span>
                </div>
                <div class="info-box-content">
                    <span class="info-box-text">
                        Pending for<br />
                        Approval
                    </span>
                    <span class="info-box-number"> @ViewBag.approved</span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>




        <!-- /.col -->
        <!-- fix for small devices only -->

        <div class="clearfix visible-sm-block"></div>

        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">

                <div id="btn">
                    <span class="info-box-icon bg-green" style="cursor: pointer;"><i class="ion ion-ios-cart-outline"></i></span>
                </div>
                <div class="info-box-content">
                    <span class="info-box-text">New Listing</span>
                    <span class="info-box-number">760</span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>
        <!-- /.col -->
        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">
                <div id="btn1">
                    <span class="info-box-icon bg-yellow" style="cursor: pointer;"><i class="ion ion-ios-people-outline"></i></span>
                </div>

                <div class="info-box-content">
                    <span class="info-box-text">Sales</span>
                    <span class="info-box-number"> @ViewBag.salescount</span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>
        <!-- /.col -->


        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">
                

                <div id="btn2">
                    <span class="info-box-icon bg-yellow" style="cursor: pointer;"><i class="ion ion-ios-cart-outline"></i></span>
                </div>


                <div class="info-box-content">
                    <span class="info-box-text">Rent</span>
                    <span class="info-box-number">@ViewBag.rentcount<small></small></span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>






        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">
               


                <div id="btn3">
                    <span class="info-box-icon bg-aqua" style="cursor: pointer;"><i class="ion ion-ios-cart-outline"></i></span>
                </div>




                <div class="info-box-content">
                    <span class="info-box-text">property Request</span>
                    <span class="info-box-number"> @ViewBag.rentcount</span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>


        <div class="col-md-3 col-sm-6 col-xs-12">
            <div class="info-box">

                <div id="btn">
                    <span class="info-box-icon bg-yellow" style="cursor: pointer;"><i class="ion ion-ios-people-outline"></i></span>
                </div>
                

                <div class="info-box-content">
                    <span class="info-box-text">
                        Enquiry of listed<br />
                        property
                    </span>
                    <span class="info-box-number"> @ViewBag.rentcount</span>
                </div>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>






        @*////////////////////////////////////*@
        <div class="row">
            <div class="col-md-12">
                <div class="box">
                    <div class="box-header with-border"  id="result">
                      
                       this is Patial view

                   </div>
               </div>
          </div>
   </div>
       
</section>
