        @using (Html.BeginForm("CreateUser", "User", FormMethod.Post))
        {

            <div class="main" role="main">
                <div id="content" class="content full">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="single-agent">
                                    <div class="block-heading">
                                        <h4>
                                            <span class="heading-icon">
                                                <img src="@Url.Content("~/images/faa.png")">
                                            </span>Register
                                        </h4>
                                    </div>
                                </div>
                                <div class="spacer-20"></div>
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                    @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                                </div>
                                <div class="col-md-4 col-sm-4 register-form">
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                        @Html.TextBoxFor(x => x.FirstName, new { @class = "form-control", @placeholder = "First Name" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                        @Html.TextBoxFor(x => x.LastName, new { @class = "form-control", @placeholder = "Last Name" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                        @Html.TextBoxFor(x => x.UserName, new { @class = "form-control", @placeholder = "UserName" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                        @Html.TextBoxFor(x => x.Email, new { @class = "form-control", @placeholder = "Email Address" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-refresh"></i></span>
                                        @Html.PasswordFor(x => x.Password, new { @class = "form-control", @placeholder = "Password" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-refresh"></i></span>
                                        @Html.PasswordFor(x => x.ConfirmPassword, new { @class = "form-control", @placeholder = "Confirm Password" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-refresh"></i></span>
                                        @Html.TextBoxFor(x => x.DateOfBirth, new { @class = "form-control", @placeholder = "Date Of Birth" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                        @Html.TextBoxFor(x => x.Phone, new { @class = "form-control", @placeholder = "Phone Number" })
                                    </div>
                                    <br>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                        <div class="col-md-12 col-sm-12 submit-gender">
                                            <select name="Gender" class="form-control ">
                                                <option value="Male">Male</option>
                                                <option value="Female">Female</option>
                                            </select>
                                        </div>
                                    </div>
                                    <br />
                                    <button type="submit" class="btn btn-success">Register</button>
                                </div>
                                <br />
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                    @ViewBag.Message
                                </div>
                            </div>
