@model IEnumerable<Kaylan.Porperty.Web.ViewModel.UserenqueryViewModel>


<style>
    .m_title:first-letter {
        text-transform: uppercase;
    }
</style>

<h3>Enquiry</h3>
<table class="table table-striped table-bordered" cellspacing="0" width="100%">
    <tr>



        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Email)</th>
        <th>@Html.DisplayNameFor(model => model.MobileNumber)</th>
        <th>@Html.DisplayNameFor(model => model.Date)</th>
        <th>@Html.DisplayNameFor(model => model.PropertyName)</th>
        <th>@Html.DisplayNameFor(model => model.Address)</th>
        <th>@Html.DisplayNameFor(model => model.AreaId)</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>


            <td class="m_title">
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropertyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AreaId)
            </td>




        </tr>
    }

</table>
    

